from abc import *
class shape(metaclass = ABCMeta):
     def printarea(self):
              return 0
class rectangle(shape):
     type1 = "rectangle"
     side = 4
     def __init__(self):
          self.length = 6
          self.breadth = 7
     def printarea(self):
          return self.length*self.breadth

rect1 = rectangle()
print(rect1.printarea())
